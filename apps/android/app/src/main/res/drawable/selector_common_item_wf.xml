<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <color android:color="#F0F0F0" />
    </item>
    <item android:state_selected="true">
        <color android:color="#F0F0F0" />
    </item>
    <item>
        <color android:color="@android:color/transparent" />
    </item>
</selector>

